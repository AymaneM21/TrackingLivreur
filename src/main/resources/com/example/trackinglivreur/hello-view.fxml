      </ImageView>
      <Button layoutX="38.0" layoutY="22.0" mnemonicParsing="false" onAction="#RetourMenu" text="Retour MENU" />
      <Button layoutX="450.0" layoutY="112.0" mnemonicParsing="false" onAction="#onDeleteButtonClick1" text="Delete" />
    </children>


</AnchorPane>
