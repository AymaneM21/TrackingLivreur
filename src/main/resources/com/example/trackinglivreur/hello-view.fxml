<AnchorPane fx:id="livreurpane" prefHeight="403.0" prefWidth="543.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trackinglivreur.LivreursController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label layoutX="22.0" layoutY="89.0" text="Nom :" />
        <TextField fx:id="nom" layoutX="23.0" layoutY="112.0" />
        <Label layoutX="205.0" layoutY="89.0" text="Telephone :" />
        <TextField fx:id="tele" layoutX="205.0" layoutY="112.0" />
        <Button fx:id="ok" layoutX="396.0" layoutY="112.0" mnemonicParsing="false" onAction="#onSaveButtonClick1" text="Save" />
        <TableView fx:id="mytable1" layoutX="80.0" layoutY="158.0" prefHeight="200.0" prefWidth="384.0">
            <columns>
                <TableColumn fx:id="col_id" prefWidth="75.0" text="id" />
                <TableColumn fx:id="col_nom" prefWidth="75.0" text="nom" />
                <TableColumn fx:id="col_tele" prefWidth="75.0" text="telephone" />

